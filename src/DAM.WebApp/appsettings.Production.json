{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }

  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=${database_hostname}$;Initial Catalog=${database_name}$;Persist Security Info=False;User ID=${database_username}$;Password=${DATABASE_PASSWORD}$;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=120;"
  },
  "Jwt": {
    "Issuer": "${DAM_WEBAPP_URL}$",
    "Audience": "${DAM_WEBAPP_URL}$",
    "EncryptionKey": "${DAM_JWT_ENCRYPTIONKEY}$"

  },
  "ApplicationInsights": {
    "ConnectionString": "${APP_INSIGHTS_CONNECTIONSTRING}$"
  },
  //Damv3
  "Discord": {
    "ClientId": "${DAM_DISCORD_CLIENTID}$",
    "ClientSecret": "${DAM_DISCORD_CLIENTSECRET}$",
    "BotToken": "${DAM_DISCORD_BOTTOKEN}$"
  },
  "Swagger": {
    "Enabled": "${SWAGGER_ENABLED}$"

  }
}
